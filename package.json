{
  "name": "prettier-style",
  "version": "0.3.0",
  "description": "prettier-style is a fool-like formatted styles tool, Support for any style of text content. (less, css, sass, wxss ....)",
  "main": "lib/prettier.js",
  "bin": {
    "prettier-style": "./bin/cli.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "nyc ava && nyc report --reporter=lcov",
    "coverage": "nyc ava && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noyobo/prettier-style.git"
  },
  "keywords": [
    "prettier",
    "wxss",
    "stylelint"
  ],
  "author": "noyobo <noyobo@gmai.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noyobo/prettier-style/issues"
  },
  "homepage": "https://github.com/noyobo/prettier-style#readme",
  "dependencies": {
    "chalk": "^4.0.0",
    "css-declaration-sorter": ">4.0.0",
    "fancy-log": "^1.3.3",
    "glob": "7.1.6",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-stylelint": "^13.0.0",
    "minimist": "^1.2.0",
    "postcss-less": "^3.1.4",
    "postcss-sass": "^0.4.4",
    "stylelint": "^15.10.1",
    "through2": "^3.0.1"
  },
  "devDependencies": {
    "ava": "3.8.2",
    "coveralls": "3.1.0",
    "execa": "4.0.2",
    "nyc": "15.1.0",
    "stylelint-config-standard": "20.0.0"
  }
}
